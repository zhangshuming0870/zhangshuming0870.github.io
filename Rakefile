# Rakefile for Jekyll project
# This file prevents Zeabur from failing when it tries to run rake commands
# Jekyll doesn't need rake tasks, but we provide empty tasks to prevent errors

namespace :assets do
  desc "Precompile assets (no-op for Jekyll)"
  task :precompile do
    puts "Jekyll project - skipping assets:precompile"
  end
end

desc "Default task (no-op for Jekyll)"
task :default do
  puts "Jekyll project - no rake tasks needed"
end

